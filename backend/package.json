{
  "name": "iot-data-marketplace-backend",
  "version": "1.0.0",
  "description": "Backend API for IoT Data Marketplace",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "init-demo": "ts-node src/scripts/init-demo-feeds.ts",
    "mock-device": "ts-node src/scripts/mock-iot-device.ts",
    "gen:key": "ts-node src/scripts/generate-private-key.ts",
    "gen:key:env": "ts-node src/scripts/generate-private-key.ts --write-env",
    "prisma:generate": "prisma generate --no-engine",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:deploy": "prisma migrate deploy"
  },
  "keywords": [
    "iot",
    "blockchain",
    "sui",
    "walrus",
    "marketplace"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mysten/sui": "^1.44.0",
    "@mysten/sui.js": "^0.54.1",
    "@prisma/client": "^6.19.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "prisma": "^6.19.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
